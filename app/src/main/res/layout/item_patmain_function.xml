<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_margin="0dip">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_centerInParent="true">

        <ImageView
            android:id="@+id/iv_vitalsign"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="1"
            android:layout_gravity="center"
            app:srcCompat="@drawable/ic_vitalsign_blue" />

        <TextView
            android:id="@+id/tv_vitalsign"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/dp_10"
            android:maxLines="1"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:text="生命体征"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <Button
            android:id="@+id/bt_vitalsignview"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:clickable="true"
            android:text="查询" />

        <Button
            android:layout_marginLeft="10dp"
            android:id="@+id/bt_vitalsigninput"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:layout_alignParentRight="true"
            android:clickable="true"
            android:text="录入" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="6dp"
        app:srcCompat="@color/grey_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_centerInParent="true">

        <ImageView
            android:id="@+id/iv_order"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="1"
            android:layout_gravity="center"
            app:srcCompat="@drawable/ic_order_blue" />

        <TextView
            android:id="@+id/tv_order"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/dp_10"
            android:maxLines="1"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:text="医嘱执行"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <Button
            android:id="@+id/bt_orderview"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:clickable="true"
            android:text="查询" />

        <Button
            android:layout_marginLeft="10dp"
            android:id="@+id/bt_orderinput"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:layout_alignParentRight="true"
            android:clickable="true"
            android:text="执行" />

    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="6dp"
        app:srcCompat="@color/grey_line" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_centerInParent="true">

        <ImageView
            android:id="@+id/iv_turnoverbody"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="1"
            android:layout_gravity="center"
            app:srcCompat="@drawable/ic_turnoverbody_green" />

        <TextView
            android:id="@+id/tv_turnoverbody"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/dp_10"
            android:maxLines="1"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:text="翻身管理"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <Button
            android:id="@+id/bt_turnoverbodyview"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:clickable="true"
            android:text="查询" />

        <Button
            android:layout_marginLeft="10dp"
            android:id="@+id/bt_turnoverbodyinput"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:layout_alignParentRight="true"
            android:clickable="true"
            android:text="操作" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="6dp"
        app:srcCompat="@color/grey_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_centerInParent="true">

        <ImageView
            android:id="@+id/iv_tour"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="1"
            android:layout_gravity="center"
            app:srcCompat="@drawable/ic_tour" />

        <TextView
            android:id="@+id/tv_tour"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/dp_10"
            android:maxLines="1"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:text="病人巡视"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <Button
            android:id="@+id/bt_tourview"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:clickable="true"
            android:text="查询" />

        <Button
            android:layout_marginLeft="10dp"
            android:id="@+id/bt_tourinput"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:layout_alignParentRight="true"
            android:clickable="true"
            android:text="巡视" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="6dp"
        app:srcCompat="@color/grey_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_centerInParent="true">

        <ImageView
            android:id="@+id/iv_nursingrecord"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="1"
            android:layout_gravity="center"
            app:srcCompat="@drawable/ic_nursingrecorder_red" />

        <TextView
            android:id="@+id/tv_nursingrecord"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/dp_10"
            android:maxLines="1"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:text="护理记录"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <Button
            android:id="@+id/bt_nursingrecordview"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:clickable="true"
            android:text="查询" />

        <Button
            android:layout_marginLeft="10dp"
            android:id="@+id/bt_nursingrecordinput"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:background="@drawable/share_button"
            android:layout_alignParentRight="true"
            android:clickable="true"
            android:text="录入" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="8dp"
        app:srcCompat="@color/grey_line" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_labcheck"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_weight="1"
                android:layout_gravity="center"
                app:srcCompat="@drawable/ic_labcheck_blue" />

            <TextView
                android:id="@+id/tv_labcheck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/dp_10"
                android:maxLines="1"
                android:layout_weight="3"
                android:layout_gravity="center"
                android:text="化验采集"
                android:textColor="@color/black"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/iv_oper"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_marginLeft="@dimen/dp_4"
                android:layout_weight="1"
                android:layout_gravity="center"
                app:srcCompat="@drawable/ic_oper_red" />

            <TextView
                android:id="@+id/tv_oper"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/dp_10"
                android:maxLines="1"
                android:layout_weight="3"
                android:layout_gravity="center"
                android:text="手术核查"
                android:textColor="@color/black"
                android:textSize="16sp" />
        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        app:srcCompat="@color/grey_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/iv_exam"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="1"
            android:layout_gravity="center"
            app:srcCompat="@drawable/ic_examreport" />

        <TextView
            android:id="@+id/tv_exam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/dp_10"
            android:maxLines="1"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:text="检查报告"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_lab"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/dp_4"
            android:layout_weight="1"
            android:layout_gravity="center"
            app:srcCompat="@drawable/ic_labreport_blue" />

        <TextView
            android:id="@+id/tv_lab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/dp_10"
            android:maxLines="1"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:text="检验报告"
            android:textColor="@color/black"
            android:textSize="16sp" />
    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="15dp"
        app:srcCompat="@color/grey_line" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="时间记录" />


</LinearLayout>